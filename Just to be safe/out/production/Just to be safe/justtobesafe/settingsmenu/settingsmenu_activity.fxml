<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="SettingsMenu" onKeyPressed="#onFunctionKeyPress" prefHeight="400.0" prefWidth="600.0" stylesheets="@../../resources/dark-theme.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="justtobesafe.settingsmenu.SettingsMenuController">
    <children>
    <Button fx:id="logoutBtn" layoutX="462.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#onLogoutButtonClicked" prefHeight="39.0" prefWidth="114.0" styleClass="logout-button" text="Logout" />
    <Button fx:id="homeBtn" layoutX="337.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#onHomeButtonClicked" prefHeight="39.0" prefWidth="114.0" styleClass="card-button" text="Home" />
    <Button fx:id="accountBtn" layoutX="337.0" layoutY="181.0" mnemonicParsing="false" onMouseClicked="#onAccountButtonClicked" prefHeight="148.0" prefWidth="239.0" styleClass="card-button" text="Accounts &amp; Passwords" />
    <Button fx:id="cardBtn" layoutX="337.0" layoutY="19.0" mnemonicParsing="false" onMouseClicked="#onCardButtonClicked" prefHeight="148.0" prefWidth="239.0" styleClass="card-button" text="Credit Cards" />
    <AnchorPane layoutX="24.0" layoutY="19.0" prefHeight="362.0" prefWidth="277.0" styleClass="upper-layer">
        <children>
            <Label layoutX="27.0" layoutY="14.0" styleClass="title-label,warning-label" text="Warning: Reset Password" />
            <Label layoutX="23.0" layoutY="72.0" text="Enter New Password:" />
            <TextField fx:id="passwd_field" layoutX="19.0" layoutY="100.0" prefHeight="39.0" prefWidth="239.0" styleClass="text-field" onMouseClicked="onTextFieldClicked" />
            <Label layoutX="23.0" layoutY="163.0" text="Re-enter New Password:" />
            <TextField fx:id="passwd_reenter_field" layoutX="20.0" layoutY="191.0" prefHeight="39.0" prefWidth="239.0" styleClass="text-field" onMouseClicked="onTextFieldClicked" />
            <Label fx:id="pswd_warning" layoutX="48.0" layoutY="252.0" prefHeight="25.0" prefWidth="180.0" styleClass="warning-label" text="" />
            <Label fx:id="pswd_warning1" layoutX="68.0" layoutY="252.0" prefHeight="25.0" prefWidth="140.0" styleClass="warning-label" text="" />
            <Label fx:id="pswd_warning2" layoutX="55.5" layoutY="252.0" prefHeight="25.0" prefWidth="165.0" styleClass="warning-label" text="" />
            <Button fx:id="passwdResetBtn" layoutX="20.0" layoutY="309.0" mnemonicParsing="false" onMouseClicked="#onPasswdResetButtonClicked" prefHeight="39.0" prefWidth="239.0" styleClass="logout-button" text="Reset Password" />
        </children>
    </AnchorPane>
    </children>
</AnchorPane>
